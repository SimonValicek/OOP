<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" styleClass="anchor" stylesheets="@../styles/homepage.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oop_project.gui.MainpageGUI">
   <children>
      <TextField fx:id="username_field" layoutX="282.0" layoutY="250.0" prefHeight="26.0" prefWidth="235.0" promptText="Username" />
      <PasswordField fx:id="password_field" layoutX="282.0" layoutY="287.0" prefHeight="26.0" prefWidth="235.0" promptText="Password" />
      <Button fx:id="sign_in_button" layoutX="373.0" layoutY="360.0" mnemonicParsing="false" onAction="#login" text="Sign In" />
      <Label layoutX="295.0" layoutY="321.0" prefHeight="18.0" prefWidth="132.0" text="Don't have an account ? " />
      <Button fx:id="sign_up_button" layoutX="445.0" layoutY="318.0" mnemonicParsing="false" onAction="#switchToRegistration" pickOnBounds="false" text="Sign up" />
      <Label fx:id="error_label" layoutX="317.0" layoutY="218.0" />
   </children>
</AnchorPane>
